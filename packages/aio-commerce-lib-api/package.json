{
  "name": "@adobe/aio-commerce-lib-api",
  "type": "module",
  "author": "Adobe Inc.",
  "version": "0.6.0",
  "private": false,
  "engines": {
    "node": ">=20 <=24"
  },
  "license": "Apache-2.0",
  "description": "A set of utilities to build HTTP/API clients for I/O Events and Adobe Commerce",
  "keywords": [],
  "bugs": {
    "url": "https://github.com/adobe/aio-commerce-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adobe/aio-commerce-sdk.git",
    "directory": "packages/aio-commerce-lib-api"
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/es/index.mjs",
  "types": "./dist/cjs/index.d.cts",
  "publishConfig": {
    "exports": {
      ".": {
        "import": {
          "types": "./dist/es/index.d.mts",
          "default": "./dist/es/index.mjs"
        },
        "require": {
          "types": "./dist/cjs/index.d.cts",
          "default": "./dist/cjs/index.cjs"
        }
      },
      "./ky": {
        "import": {
          "types": "./dist/es/ky.d.mts",
          "default": "./dist/es/ky.mjs"
        },
        "require": {
          "types": "./dist/cjs/ky.d.cts",
          "default": "./dist/cjs/ky.cjs"
        }
      },
      "./utils": {
        "import": {
          "types": "./dist/es/utils/index.d.mts",
          "default": "./dist/es/utils/index.mjs"
        },
        "require": {
          "types": "./dist/cjs/utils/index.d.cts",
          "default": "./dist/cjs/utils/index.cjs"
        }
      },
      "./package.json": "./package.json"
    }
  },
  "exports": {
    ".": "./source/index.ts",
    "./ky": "./source/ky.ts",
    "./utils": "./source/utils/index.ts",
    "./package.json": "./package.json"
  },
  "imports": {
    "#*": "./source/*.ts",
    "#test/*": "./test/*.ts"
  },
  "files": [
    "dist",
    "package.json",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown",
    "pack": "pnpm pack",
    "publint": "publint",
    "docs": "typedoc && prettier --write '**/*.md'",
    "assist": "biome check --formatter-enabled=false --linter-enabled=false --assist-enabled=true --no-errors-on-unmatched",
    "assist:apply": "biome check --write --formatter-enabled=false --linter-enabled=false --assist-enabled=true --no-errors-on-unmatched",
    "check:ci": "biome ci --formatter-enabled=true --linter-enabled=true --assist-enabled=true --no-errors-on-unmatched",
    "format": "biome format --write --no-errors-on-unmatched",
    "format:markdown": "prettier --no-error-on-unmatched-pattern --write '**/*.md' \"!**/{CODE_OF_CONDUCT.md,COPYRIGHT,LICENSE,SECURITY.md,CONTRIBUTING.md}\"",
    "format:check": "biome format --no-errors-on-unmatched",
    "lint": "biome lint --no-errors-on-unmatched",
    "lint:fix": "biome lint --write --no-errors-on-unmatched",
    "typecheck": "tsc --noEmit && echo 'âœ… No type errors found.'",
    "test": "vitest run --coverage",
    "test:watch": "vitest --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@adobe/aio-commerce-lib-auth": "workspace:*",
    "@adobe/aio-commerce-lib-core": "workspace:*",
    "camelcase": "^8.0.0",
    "ky": "^1.9.0"
  },
  "devDependencies": {
    "@aio-commerce-sdk/config-tsdown": "workspace:*",
    "@aio-commerce-sdk/config-typedoc": "workspace:*",
    "@aio-commerce-sdk/config-typescript": "workspace:*",
    "@aio-commerce-sdk/config-vitest": "workspace:*"
  },
  "lint-staged": {
    "*.{ts,d.ts,d.cts,d.mts,tsx}": [
      "pnpm run typecheck --filter @adobe/aio-commerce-lib-api"
    ]
  },
  "sideEffects": false
}
